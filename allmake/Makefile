# GNU Make solution makefile autogenerated by Premake
# Type "make help" for usage help

ifndef config
  config=debug
endif
export config

PROJECTS := MyProject1 MyProject2

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

MyProject1: 
	@echo "==== Building MyProject1 ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f MyProject1.make

MyProject2: 
	@echo "==== Building MyProject2 ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f MyProject2.make

clean:
	@${MAKE} --no-print-directory -C . -f MyProject1.make clean
	@${MAKE} --no-print-directory -C . -f MyProject2.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo "   debug64"
	@echo "   release64"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   MyProject1"
	@echo "   MyProject2"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
